
<view class="container"> 
 <view class="login-icon"> 
 <text>用户信息认证</text>
 </view> 
 <view class="login-from"> 
  
 <!--账号-->
 <view class="inputView"> 
  <image class="nameImage" src="../../../images/bottomNav/mail.jpg"></image> 
  <label class="loginLab">邮箱</label> 
  <input class="inputText" placeholder="请输入邮箱" bindinput="phoneInput" /> 
 </view> 
 <view class="line"></view> 
  
